{
  "name": "magpie",
  "private": true,
  "version": "1.6.2",
  "description": "Metadata Assignment GUI Providing Import and Export",
  "repository": "https://github.com/TAMULib/MetadataAssignmentToolUI",
  "license": "MIT",
  "scripts": {
    "pretest": "npm install",
    "postinstall": "grunt",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "test-ci": "karma start karma.conf.js --no-auto-watch --single-run --browsers=ChromeHeadlessNoSandbox",
    "posttest-ci": "grunt coverage",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e/protractor.conf.js",
    "stop": "wvr server --stop",
    "start": "wvr server -b /magpie --prod --uid magpie-webserver",
    "start-dev": "wvr server -b /magpie --uid magpie-webserver"
  },
  "dependencies": {
    "ng-csv": "0.3.6",
    "ng-file-upload": "12.2.13",
    "ng-openseadragon": "1.3.5",
    "ng-table": "3.0.1",
    "openseadragon": "2.4.1",
    "@wvr/core": "2.0.7",
    "webdriver-manager": "^12.1.8"
  },
  "devDependencies": {
    "grunt": "1.3.0",
    "grunt-cli": "1.3.2",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-jshint": "2.1.0",
    "grunt-contrib-symlink": "1.0.0",
    "grunt-contrib-uglify": "4.0.1",
    "grunt-contrib-watch": "1.1.0",
    "grunt-karma-coveralls": "2.5.4",
    "grunt-sass": "3.1.0",
    "grunt-usemin": "3.1.1",
    "http-server": "0.12.1",
    "jasmine-core": "3.5.0",
    "jasmine-promise-matchers": "2.6.0",
    "jshint-stylish": "2.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.1",
    "karma-firefox-launcher": "1.3.0",
    "karma-jasmine": "3.1.1",
    "karma-junit-reporter": "2.0.1",
    "karma-ng-html2js-preprocessor": "1.0.0",
    "node-sass": "4.13.1",
    "protractor": "5.4.3"
  }
}
